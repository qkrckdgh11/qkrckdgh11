import random #random 도구상자를 내 코드에 불러오겠다.

aList=[] #비어있는 리스트 만들기

#1~5사이의 임의의 숫자를 a에 추가
aList.append(random.randint(1,9))
while len(aList)<3:
    r=random.randint(1,9)
