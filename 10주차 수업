    import random

a=list(range(1,101)   ) #1~100사이의 숫자를 리스트로 만들기


random.shuffle(a) #a리스트 섞기

a = a[0:3]  # 인덱스 0, 1, 2 번 만 a리스트로 하기
s,b,o=0,0,0

n1,n2,n3 = input('숫자 3개 입력(띄어쓰기구분)').spilt(' ')
n1 = int(n1)
n2 = int(n2)
n3 = int(n3)


#플레이어에게 알려줄 힌트 처리하기
if n1 in a:
    if n1==a[0]:
        s+=1
    else:
        b+=1

else:
    o+=1
